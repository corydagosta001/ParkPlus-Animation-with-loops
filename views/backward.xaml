<UserControl x:Class="Animation.views.backward"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Animation.views"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="412.534"
             Name="bWin" Loaded="bWin_Loaded">
    <Grid Name="backGrid" Margin="0,0,0,-107" >
        <StackPanel Name="BackArrow" HorizontalAlignment="Left"  Height="163" Margin="151,25,0,0" VerticalAlignment="Top" Width="100" RenderTransformOrigin="0.523,0.498">
            <StackPanel.Background>
                <SolidColorBrush Color="White"/>
            </StackPanel.Background>
            <!--<Image x:Name="ImageViewer1" Source="{Binding DisplayedArrow}" Height="100"  Margin="8,0,7,0" />-->
            <Image x:Name="ImageViewer1" Source="{Binding DisplayedArrow}" />
        </StackPanel>
    </Grid>
</UserControl>
